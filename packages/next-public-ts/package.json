{
  "name": "next-public-ts",
  "version": "0.0.4",
  "description": "A webpack plugin to compile TypeScript files in the public folder of a Next.js project.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "license": "MIT",
  "dependencies": {
    "next": ">=14.0.0",
    "webpack": ">=5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yovach/next-public-ts.git"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
